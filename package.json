{
  "name": "eslint-config-braintree",
  "version": "6.0.0-typescript-prep-rc.1",
  "description": "Braintree's ESLint configuration",
  "files": [
    "dist/",
    "*.js"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "prettier --write .",
    "build": "tsc --declaration",
    "prelint": "npm run build",
    "lint": "eslint --ext .js,.ts .",
    "posttest": "npm run lint",
    "test": "jest"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braintree/eslint-config.git"
  },
  "peerDependencies": {
    "eslint": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "eslint": "^8.1.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "bugs": {
    "url": "https://github.com/braintree/eslint-config/issues"
  },
  "homepage": "https://github.com/braintree/eslint-config#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "author": "",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
